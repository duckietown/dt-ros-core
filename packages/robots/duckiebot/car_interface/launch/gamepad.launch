<?xml version="1.0"?>
<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="pkg_name" default="joy"/>
    <arg name="node_name" default="joy_node"/>

    <group ns="$(arg veh)">
        <node pkg ="$(arg pkg_name)" type="$(arg node_name)" name="$(arg node_name)" output="screen" />
    </group>
</launch>
